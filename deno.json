{
  "name": "@negrel/http-ece",
  "version": "0.5.0",
  "exports": "./mod.ts",
  "compilerOptions": {
    "allowJs": false,
    "strict": true
  },
  "lint": {
    "include": [
      "."
    ],
    "exclude": [
      "dist/",
      "npm/"
    ],
    "rules": {
      "tags": [
        "recommended"
      ]
    }
  },
  "fmt": {
    "include": [
      "."
    ],
    "exclude": [
      "dist/",
      "npm/"
    ],
    "indentWidth": 2
  },
  "publish": {
    "include": [
      "*.ts",
      "README.md",
      "deno.lock",
      "deno.json"
    ],
    "exclude": [
      "*_test.ts"
    ]
  },
  "imports": {
    "@deno/dnt": "jsr:@deno/dnt@0.38.0",
    "@std/encoding": "jsr:@std/encoding@0.224.0",
    "@std/bytes": "jsr:@std/bytes@0.224.0",
    "@std/assert": "jsr:@std/assert@0.224.0"
  }
}
